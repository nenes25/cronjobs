{
    "name": "hhennes/prestashop-cronjobs",
    "description": "Fork of legacy PrestaShop module cronjobs",
    "homepage": "https://github.com/nenes25/cronjobs",
    "license": "AFL-3.0",
    "authors": [
        {
            "name": "PrestaShop SA",
            "email": "contact@prestashop.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "preferred-install": "dist"
    },
    "type": "prestashop-module",
    "require-dev": {
        "prestashop/php-dev-tools": "^4.3"
    },
    "scripts": {
        "set-license-header": [
            "@php ./vendor/bin/header-stamp --license=\"vendor/prestashop/header-stamp/assets/afl.txt\" --exclude=\".github,node_modules,vendor\""
        ],
      "cs-fixer": [
            "@php ./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
        ]
    }
}
